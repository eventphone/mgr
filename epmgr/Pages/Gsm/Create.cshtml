@page
@model epmgr.Pages.Gsm.CreateModel

@{
    ViewBag.Title = "Register SIM";
}

<h2>Register IMSI</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input class="form-control" asp-for="Name">
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Imsi"></label>
        <input class="form-control" asp-for="Imsi">
        <span asp-validation-for="Imsi" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
